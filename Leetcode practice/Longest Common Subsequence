class Solution {
    public int lengthOfLIS(int[] nums) {
        if(nums == null || nums.length == 0){
            return 0;
        }
        int[] dp = new int[nums.length];
        int len = 0;
        for (int num : nums) {
            int index = Arrays.binarySearch(dp, 0, len, num);
            if(index < 0)
                index = -(index + 1);
            dp[index] = num;
            if(index == len)
                ++len;
        }
        return len;
        
    }
}
